				},
				"core:sample_rate": {
					"type": "number"
				},
				"core:version": {
					"type": "string"
				},
				"core:num_channels": {
					"type": "integer"
				},
				"core:sha512": {
					"type": "string"
				},
				"core:offset": {
					"type": "integer"
				},
				"core:description": {
					"type": "string"
				},
				"core:author": {
					"type": "string"
				},
				"core:meta_doi": {
					"type": "string"
				},
				"core:data_doi": {
					"type": "string"
				},
				"core:recorder": {
					"type": "string"
				},
				"core:license": {
					"type": "string"
				},
				"core:hw": {
					"type": "string"
				},

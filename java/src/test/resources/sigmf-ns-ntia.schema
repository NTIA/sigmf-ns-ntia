				"core:header_bytes": {
					"type": "integer"
				},
				"core:frequency": {
					"type": "number"
				},
				"core:datetime": {
					"type": "string",
					"format": "date-time"
				},
				"ntia-sensor:duration": {
					"type": "integer"
				},
				"ntia-sensor:overload": {
					"type": "boolean"
				},
				"ntia-sensor:sensor_calibration": {
					"$ref": "#/$defs/Calibration"
				},
				"ntia-sensor:sigan_calibration": {
					"$ref": "#/$defs/Calibration"
				},
				"ntia-sensor:sigan_settings": {
					"$ref": "#/$defs/SiganSettings"
				}
			},

				"core:sample_start"
			]
		},
		"Annotation": {
			"type": "object",
			"properties": {
				"core:sample_start": {
					"type": "integer"
				},
				"core:sample_count": {
					"type": "integer"
				},
				"core:generator": {
					"type": "string"
				},
				"core:comment": {
					"type": "string"
				},
				"core:freq_lower_edge": {
					"type": "number"
				},
				"core:freq_upper_edge": {
					"type": "number"
				},
				"uuid": {
					"type": "string"
				}
			},
			"required": [
				"core:sample_start"
			]
		},
		"GeoJSONPoint": {
			"type": "object",
			"properties": {
				"type": {
					"type": "string"
				},
				"coordinates": {
					"type": "array",
					"items": {
						"type": "number"
					},
					"minItems": 2,
					"maxItems": 3
				}
			},
			"required": [
				"type",
				"coordinates"
			]
		},
		"Extension": {
			"type": "object",
			"properties": {
				"name": {
					"type": "string"
				},
				"version": {
					"type": "string"
				},
				"optional": {
					"type": "boolean"
				}
			},
			"additionalProperties": false,
			"required": [
				"name",
				"version",
				"optional"
			]
		},
		"HardwareSpec": {

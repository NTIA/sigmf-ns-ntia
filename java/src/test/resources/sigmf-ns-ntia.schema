					"type": "array",
					"items": {
						"type": "number"
					}
				},
				"ntia-nasctn-sea:median_of_mean_channel_powers": {
					"type": "array",
					"items": {
						"type": "number"
					}
				},
				"ntia-algorithm:processing_info": {
					"type": "array",
					"items": {
						"anyOf": [{
								"$ref": "#/$defs/DFT"
							},
							{
								"$ref": "#/$defs/DigitalFilter"
							}
						]
					}
				},
				"ntia-algorithm:processing": {
					"type": "array",
					"items": {
						"type": "string"
					}
				},
				"ntia-scos:schedule": {
					"$ref": "#/$defs/ScheduleEntry"

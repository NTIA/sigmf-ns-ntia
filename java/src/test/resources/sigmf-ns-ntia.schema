				"horizontal_gain_pattern": {
					"type": "array",
					"items": {
						"type": "number"
					}
				},
				"vertical_gain_pattern": {
					"type": "array",
					"items": {
						"type": "number"
					}
				},
				"horizontal_beamwidth": {
					"type": "number"
